#include <vector>
#include <string>
#include <unordered_set>
â€‹
std::vector<int> vowelIndices(const std::string& word)
{
    std::vector<int> res;
    std::unordered_set<char> vowels = {'a', 'e', 'i', 'o', 'u', 'A', 'E', 'I', 'O', 'U', 'y', 'Y'};
    int n = word.size();
  
    for(int i = 0; i < n; i++) {
      if(vowels.count(word[i])){
        res.push_back(i + 1);
      }
    }
        
    return res;
}